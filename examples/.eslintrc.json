{
    "extends": "airbnb",
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/jsx-filename-extension": "off",
      "semi": ["error", "never"],
      "react/prop-types": "off",
      "react/destructuring-assignment": "off",
      "import/no-extraneous-dependencies": "off"
    }
}